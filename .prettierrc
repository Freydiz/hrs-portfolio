{
  "jsxSingleQuote": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrder": ["^react$", "^next(/.*)?$", "next-themes", "@/*", "<THIRD_PARTY_MODULES>"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
